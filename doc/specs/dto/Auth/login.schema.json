{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "description": "Модель для авторизации пользователя.",
    "required": [
        "login",
        "password"
    ],
    "properties": {
        "login": {
            "type": "string",
            "description": "Логин пользователя."
        },
        "password": {
            "type": "string",
            "description": "Пароль пользователя."
        }
    }
}