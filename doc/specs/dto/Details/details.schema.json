{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "description": "Результат поиска деталей по фильтрам.",
    "required": [
      "items",
      "count"
    ],
    "properties": {
      "items": {
        "type": "array",
        "items": {
          "$ref": "detail.schema.json"
        },
        "description": "Детали, найденные по фильтрам."
      },
      "count": {
        "type": "integer",
        "description": "Количество деталей, найденных по фильтрам."
      }
    }
}